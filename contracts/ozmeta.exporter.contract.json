        {
          "name": "WF_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "WS_Code",
          "type": "nvarchar(120)"
        },
        {
          "name": "WS_IsStart",
          "type": "bit"
        },
        {
          "name": "WS_IsTerminal",
          "type": "bit"
        },
        {
          "name": "WS_TextKeyID",
          "type": "uniqueidentifier"
        },
        {
          "name": "WS_Notes",
          "type": "nvarchar(1000)"
        }
