        {
          "name": "WF_ObjectType",
          "type": "nvarchar(30)"
        },
        {
          "name": "WF_ObjectID",
          "type": "uniqueidentifier"
        },
        {
          "name": "WF_TextKeyID",
          "type": "uniqueidentifier"
        },
        {
          "name": "WF_IsEnabled",
          "type": "bit"
