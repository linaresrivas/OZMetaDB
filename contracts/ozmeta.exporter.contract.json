{
  "version": 1,
  "name": "ozmeta.exporter.contract",
  "description": "Deterministic contract for SQL exporter queries to produce snapshot JSON.",
  "dialect": "TSQL",
  "principles": [
    "Each query returns a stable set of columns with stable names and types.",
    "No secrets in results.",
    "Filter by (CL_Code, PJ_Code) and _DeleteDate IS NULL by default.",
    "Order results deterministically by primary natural keys."
  ],
  "parameters": {
    "CL_Code": "nvarchar(50)",
    "PJ_Code": "nvarchar(80)"
  },
  "queries": [
    {
      "name": "client_project_context",
      "purpose": "Resolve CL_ID and PJ_ID and versions (optional).",
      "sql": "SELECT c.CL_ID, c.CL_Code, p.PJ_ID, p.PJ_Code FROM Meta.Client c JOIN Meta.Project p ON p.CL_ID=c.CL_ID WHERE c.CL_Code=@CL_Code AND p.PJ_Code=@PJ_Code AND c._DeleteDate IS NULL AND p._DeleteDate IS NULL;",
      "output": [
        {
          "name": "CL_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "CL_Code",
          "type": "nvarchar(50)"
        },
        {
          "name": "PJ_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "PJ_Code",
          "type": "nvarchar(80)"
        }
      ],
      "orderBy": [
        "CL_Code",
        "PJ_Code"
      ]
    },
    {
      "name": "tables",
      "purpose": "Canonical tables for project.",
      "sql": "SELECT TB_ID, PJ_ID, TB_SchemaName, TB_TableName, CD_Code, TB_TableType, TB_TextKeyID, TB_Description, TB_IsActual, TB_IsForecast, TB_RequiresTenant, TB_SoftDeleteEnabled, TB_SourceTrackingEnabled FROM Meta.Table WHERE PJ_ID=@PJ_ID AND _DeleteDate IS NULL ORDER BY TB_SchemaName, TB_TableName;",
      "output": [
        {
          "name": "TB_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "PJ_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "TB_SchemaName",
          "type": "nvarchar(128)"
        },
        {
          "name": "TB_TableName",
          "type": "nvarchar(128)"
        },
        {
          "name": "CD_Code",
          "type": "nchar(2)"
        },
        {
          "name": "TB_TableType",
          "type": "nvarchar(30)"
        },
        {
          "name": "TB_TextKeyID",
          "type": "uniqueidentifier"
        },
        {
          "name": "TB_Description",
          "type": "nvarchar(2000)"
        },
        {
          "name": "TB_IsActual",
          "type": "bit"
        },
        {
          "name": "TB_IsForecast",
          "type": "bit"
        },
        {
          "name": "TB_RequiresTenant",
          "type": "bit"
        },
        {
          "name": "TB_SoftDeleteEnabled",
          "type": "bit"
        },
        {
          "name": "TB_SourceTrackingEnabled",
          "type": "bit"
        }
      ],
      "orderBy": [
        "TB_SchemaName",
        "TB_TableName"
      ]
    },
    {
      "name": "fields",
      "purpose": "Canonical fields for project tables.",
      "sql": "SELECT f.FD_ID, f.TB_ID, f.FD_FieldName, f.LT_Code, f.FD_IsPK, f.FD_IsFK, f.FD_IsInternal, f.FD_IsNullable, f.FD_DefaultRule, f.FD_Sensitivity, f.FD_TextKeyID, e.EN_Code AS EnumCode, f.FD_Format, f.FD_Mask, f.FD_Description FROM Meta.Field f JOIN Meta.Table t ON t.TB_ID=f.TB_ID AND t._DeleteDate IS NULL LEFT JOIN Meta.Enum e ON e.EN_ID=f.FD_EnumID AND e._DeleteDate IS NULL WHERE t.PJ_ID=@PJ_ID AND f._DeleteDate IS NULL ORDER BY t.TB_SchemaName, t.TB_TableName, f.FD_FieldName;",
      "output": [
        {
          "name": "FD_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "TB_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "FD_FieldName",
          "type": "nvarchar(128)"
        },
        {
          "name": "LT_Code",
          "type": "nvarchar(50)"
        },
        {
          "name": "FD_IsPK",
          "type": "bit"
        },
        {
          "name": "FD_IsFK",
          "type": "bit"
        },
        {
          "name": "FD_IsInternal",
          "type": "bit"
        },
        {
          "name": "FD_IsNullable",
          "type": "bit"
        },
        {
          "name": "FD_DefaultRule",
          "type": "nvarchar(400)"
        },
        {
          "name": "FD_Sensitivity",
          "type": "nvarchar(50)"
        },
        {
          "name": "FD_TextKeyID",
          "type": "uniqueidentifier"
        },
        {
          "name": "EnumCode",
          "type": "nvarchar(120)"
        },
        {
          "name": "FD_Format",
          "type": "nvarchar(80)"
        },
        {
          "name": "FD_Mask",
          "type": "nvarchar(80)"
        },
        {
          "name": "FD_Description",
          "type": "nvarchar(2000)"
        }
      ],
      "orderBy": [
        "TB_SchemaName",
        "TB_TableName",
        "FD_FieldName"
      ]
    },
    {
      "name": "relations",
      "purpose": "Relations between canonical fields.",
      "sql": "SELECT RL_ID, RL_FromFD_ID, RL_ToFD_ID, RL_RoleName, RL_Cardinality, RL_EnforceInDB, RL_TextKeyID, RL_Description FROM Meta.Relation WHERE _DeleteDate IS NULL AND (RL_FromFD_ID IN (SELECT FD_ID FROM Meta.Field f JOIN Meta.Table t ON t.TB_ID=f.TB_ID WHERE t.PJ_ID=@PJ_ID AND t._DeleteDate IS NULL AND f._DeleteDate IS NULL));",
      "output": [
        {
          "name": "RL_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "RL_FromFD_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "RL_ToFD_ID",
          "type": "uniqueidentifier"
        },
        {
          "name": "RL_RoleName",
          "type": "nvarchar(100)"
        },
        {
          "name": "RL_Cardinality",
          "type": "nvarchar(20)"
        },
        {
          "name": "RL_EnforceInDB",
          "type": "bit"
        },
        {
          "name": "RL_TextKeyID",
          "type": "uniqueidentifier"
        },
        {
          "name": "RL_Description",
          "type": "nvarchar(1000)"
        }
      ],
      "orderBy": [
        "RL_ID"
      ]
    },
    {
      "name": "texts",
      "purpose": "Languages, TextKeys, Translations used in project scope.",
      "sql": "SELECT LG_Code, LG_Name, LG_IsDefault FROM Meta.Language WHERE _DeleteDate IS NULL AND LG_IsEnabled=1 ORDER BY LG_IsDefault DESC, LG_Code;",
      "output": [
        {
          "name": "LG_Code",
          "type": "nvarchar(20)"
        },
        {
          "name": "LG_Name",
          "type": "nvarchar(100)"
        },
        {
          "name": "LG_IsDefault",
          "type": "bit"
        }
      ],
      "orderBy": [
        "LG_IsDefault DESC",
        "LG_Code"
      ]
    },
    {
      "name": "workflows",
      "purpose": "Workflows for project.",
